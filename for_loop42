#include <iostream>

int funkcja1()
{
    return 42;
}

void funkcja2()
{
    for (int i=0; i<42; ++i)
    {
        std::cout << 42;
    }
    std::cout << std::endl;
}

int main()
{
    funkcja2();
    std::cout << "Hello, World! Odpowiedz na wszystkie pytania to: " << funkcja1();
    funkcja2();
}
